{
  "name": "security-dashboard-backend",
  "version": "1.0.0",
  "description": "Enterprise Security Dashboard Backend with LDAP, PostgreSQL, Redis, and Security Tool Integrations",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/workers/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/workers/index.js",
    "vercel-build": "npm run build",
    "migrate": "psql -U $DB_USER -d $DB_NAME -f src/database/schema.sql",
    "seed": "node dist/database/seed.js"
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": ["security", "dashboard", "soc", "siem", "enterprise"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "ldapjs": "^3.0.7",
    "bullmq": "^5.1.0",
    "ws": "^8.16.0",
    "axios": "^1.6.5",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "@types/ldapjs": "^3.0.6",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  }
}
